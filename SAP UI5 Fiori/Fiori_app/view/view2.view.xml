<mvc:View xmlns:mvc="sap.ui.core.mvc"
		  controllerName="fiori.controller.view2"
		  xmlns:core="sap.ui.core"
		  xmlns="sap.m"
		  xmlns:tab="sap.ui.table">
	
	<Page title="View 2" showNavButton="true" navButtonPress="vi2">
		<headerContent>
			<Button text="Go to view 3" press="vi3" icon="sap-icon://media-forward"></Button>
		</headerContent>
		
		<content>
			<Button text="Go to view 1" press="vi2" icon="sap-icon://media-rewind"></Button>	
			<Image src="data:image/jpeg;base64,"></Image>
			<Button text="Go to view 3" press="vi3" icon="sap-icon://media-forward"></Button>	
		</content>
		
		<content>
			<Title text="City List function"></Title>
			<SearchField search="onSearch" liveChange="onSearch"></SearchField>
			<List id="myList" items="{/city}" mode="SingleSelectMaster" selectionChange="onPressItem" >
				<items>
					<ObjectListItem intro="{state}" title="{cityName}" icon="{pic}">
						<attributes>
							<ObjectAttribute title="Famous for" text="{famousFor}"></ObjectAttribute>
						</attributes>
						<firstStatus>
							<ObjectStatus text="{remarks}" state="Warning"></ObjectStatus>
						</firstStatus>
					</ObjectListItem>
				</items> 
			</List> 
		</content> 
		
		<content>
			<tab:Table rows="{/city}" visibleRowCount="5" selectionMode="Single" visible="false">
				<tab:columns>
					<tab:Column>
						<tab:label>
							<Label text="City name"></Label>
						</tab:label>
						<tab:template>
							<Text text="{cityName}"></Text>
						</tab:template>
					</tab:Column>
					
					<tab:Column>
						<tab:label>
							<Label text="State"></Label>
						</tab:label>
						<tab:template>
							<Text text="{state}"></Text>
						</tab:template>
					</tab:Column>
					
					<tab:Column>
						<tab:label>
							<Label text="Famous for"></Label>
						</tab:label>
						<tab:template>
							<Text text="{famousFor}"></Text>
						</tab:template>
					</tab:Column>	
					
					<tab:Column>
						<tab:label>
							<Label text="Remarks"></Label>
						</tab:label>
						<tab:template>
							<Text text="{remarks}"></Text>
						</tab:template>
					</tab:Column>
					
					<tab:Column>
						<tab:label>
							<Label text="Pic"></Label>
						</tab:label>
						<tab:template>
							<Image src="{pic}" width="100px" height="100px"></Image>
						</tab:template>
					</tab:Column>
				</tab:columns>
			</tab:Table>
		</content>
		
		<footer>
			<Toolbar></Toolbar>
		</footer>
		
	</Page>
	
</mvc:View>
